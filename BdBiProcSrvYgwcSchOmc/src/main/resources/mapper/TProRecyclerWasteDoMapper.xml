<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tfit.BdBiProcSrvYgwcSchOmc.dao.mapper.TProRecyclerWasteDoMapper">
  
  <select id="getAllRecyclerIdName" resultType="com.tfit.BdBiProcSrvYgwcSchOmc.dao.domain.TProRecyclerWasteDo">
    select distinct recycler_id recyclerId, recycler_name recyclerName
    from t_pro_recycler_waste
    where stat = 1
    order by recycler_id asc
  </select>
  
  <select id="getAllRecPersonName" resultType="com.tfit.BdBiProcSrvYgwcSchOmc.dao.domain.TProRecyclerWasteDo">
    select distinct contact
    from t_pro_recycler_waste
    where stat = 1
  </select>
  
</mapper>